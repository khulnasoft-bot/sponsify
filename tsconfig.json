{
    "compilerOptions": {
      "target": "es2017",
      "lib": ["esnext"],
      "module": "esnext",
      "moduleResolution": "node",
      "paths": {
        "sponsify": [
          "./src/index.ts"
        ]
      },
      "resolveJsonModule": true,
      "strict": true,
      "strictNullChecks": true,
      "esModuleInterop": true,
