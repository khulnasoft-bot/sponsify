{
  "name": "@khulnasoft/sponsify-utils",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.1.0",
  "description": "Opinionated collection of common JavaScript / TypeScript utils by @khulnasoft",
  "author": "Md Sulaiman <dev.sulaiman@icloud.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/khulnasoft",
  "homepage": "https://github.com/khulnasoft-bot/sponsify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khulnasoft-bot/sponsify.git"
  },
  "bugs": {
    "url": "https://github.com/khulnasoft-bot/sponsify/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "lint-fix": "nr lint --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@khulnasoft/eslint-config": "workspace:*",
    "@khulnasoft/ni": "^0.0.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/node": "^20.17.6",
    "@types/throttle-debounce": "^5.0.2",
    "bumpp": "^9.8.1",
    "eslint": "npm:eslint-ts-patch@8.55.0-1",
    "eslint-ts-patch": "8.55.0-1",
    "esno": "^4.8.0",
    "p-limit": "^5.0.0",
    "rollup": "^4.24.4",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "throttle-debounce": "5.0.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^1.6.0"
  }
}
